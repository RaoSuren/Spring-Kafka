package codinpad.model;

public class Response {
    public Integer id;
    public String response;

    public Response(int id, String response)
    {
      this.id = id;
      this.response = response;
    }

    public String getResponse()
    {
      return response;
    }
    public void setResponse(String response)
    {
      this.response = response;
    }
}